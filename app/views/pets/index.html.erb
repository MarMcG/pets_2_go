<h1>Pets#index</h1>
<p>Find me in app/views/pets/index.html.erb</p>

<ul>
  <% @pets.each do |pet|%>
    <li>
      <%= pet.id %>
      <%= pet.name %>
      <%= link_to 'Show', pet %>
      <%= link_to "destroy_pet", pet_path(pet), method: :delete, data: { confirm: "You sure?" } %>
    </li>
  <% end %>
</ul>

<%= link_to"new pet", new_pet_path %>
